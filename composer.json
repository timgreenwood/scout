{
    "name": "timgreenwood/scout",
    "description": "Tim Greenwood Scout - Interact with Cloudabove Scout from within Laravel.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Greenwood",
            "email": "tim@tgmedia.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Timgreenwood\\Scout\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
